<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.main.templates.UIControllers.LogIn.LoginController">
   <children>
      <Canvas fx:id="background" height="401.0" width="601.0" />
      <HBox fx:id="mainPanel" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: none;">
         <children>
            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0">
               <children>
                  <Label text="Login" textFill="#f0a818">
                     <font>
                        <Font name="Poppins Bold" size="24.0" />
                     </font>
                  </Label>
                  <VBox maxHeight="250.0" minHeight="200.0" prefHeight="200.0" prefWidth="300.0" spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="EMAIL" textFill="WHITE">
                           <font>
                              <Font name="Poppins Regular" size="12.0" />
                           </font></Label>
                        <TextField fx:id="emailF" prefHeight="24.0" prefWidth="189.0" promptText="example@gmail.com" style="-fx-border-width: 0px; -fx-border-radius: 5px; -fx-text-fill: black;">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                           <font>
                              <Font name="Poppins Regular" size="13.0" />
                           </font>
                        </TextField>
                        <Label layoutX="10.0" layoutY="10.0" text="PASSWORD" textFill="WHITE">
                           <font>
                              <Font name="Poppins Regular" size="12.0" />
                           </font>
                        </Label>
                        <PasswordField fx:id="passF" promptText="******" style="-fx-border-width: 0px; -fx-border-radius: 5px; -fx-text-fill: black;">
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </PasswordField>
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                     </padding>
                  </VBox>
                  <Button fx:id="loginBtn" mnemonicParsing="false" style="-fx-background-color: #f0a818; -fx-border-radius: 40px; -fx-cursor: hand;" text="Sign Up">
                     <font>
                        <Font name="Poppins Bold" size="15.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                     </padding>
                  </Button>
                  <Label fx:id="registerLbl" onMouseClicked="#changeToRegisterPage" style="-fx-cursor: hand;" text="Do you have an account?" textFill="WHITE" underline="true">
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </Label>
               </children>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </VBox>
         </children></HBox>

   </children>
</StackPane>
